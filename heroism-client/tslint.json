{
  "extends": "tslint-config-standard-plus",
  "rules": {
    "quotemark": [
      true,
      "double",
      "avoid-escape"
    ],
    "space-before-function-paren": false,
    "indent": [
      true,
      "spaces",
      4
    ],
    "ter-indent": [
      false
    ],
    "semicolon": [
      true,
      "always"
    ],
    "trailing-comma": [
      true,
      "always"
    ]
  }
}
